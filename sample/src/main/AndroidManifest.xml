<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.colorchen">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:theme="@style/AppTheme"
        android:label="@string/app_name">
        <activity android:name=".photoview.sample.LauncherActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".photoview.sample.SimpleSampleActivity"/>
        <activity android:name=".photoview.sample.ViewPagerActivity"/>
        <activity android:name=".photoview.sample.RotationSampleActivity"/>
        <activity android:name=".photoview.sample.PicassoSampleActivity"/>
        <activity android:name=".photoview.sample.demo.SynthesizeDemo"/>
    </application>

</manifest>
